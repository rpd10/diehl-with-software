{
  "version": 1,
  "projects": {
    "web": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/web",
      "sourceRoot": "apps/web/src",
      "prefix": "web",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/web",
            "index": "apps/web/src/index.html",
            "main": "apps/web/src/main.ts",
            "polyfills": "apps/web/src/polyfills.ts",
            "tsConfig": "apps/web/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/web/src/favicon.ico",
              "apps/web/src/assets"
            ],
            "styles": [
              "apps/web/src/styles.scss",
              "apps/web/src/theme.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/web/src/environments/environment.ts",
                  "with": "apps/web/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          },
          "outputs": [
            "{options.outputPath}"
          ]
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "web:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "web:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "web:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/web/tsconfig.app.json",
              "apps/web/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!apps/web/**/*"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/web/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": [
            "coverage/apps/web"
          ]
        }
      }
    },
    "web-e2e": {
      "root": "apps/web-e2e",
      "sourceRoot": "apps/web-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/web-e2e/cypress.json",
            "tsConfig": "apps/web-e2e/tsconfig.e2e.json",
            "devServerTarget": "web:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "web:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/web-e2e/tsconfig.e2e.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!apps/web-e2e/**/*"
            ]
          }
        }
      }
    },
    "home": {
      "projectType": "library",
      "root": "libs/home",
      "sourceRoot": "libs/home/src",
      "prefix": "web",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/home/tsconfig.lib.json",
              "libs/home/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!libs/home/**/*"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/home/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": [
            "coverage/libs/home"
          ]
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      }
    },
    "blog": {
      "projectType": "library",
      "root": "libs/blog",
      "sourceRoot": "libs/blog/src",
      "prefix": "rpd10",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/blog/tsconfig.lib.json",
              "libs/blog/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!libs/blog/**/*"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/blog/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": [
            "coverage/libs/blog"
          ]
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      }
    },
    "tags": {
      "projectType": "library",
      "root": "libs/tags",
      "sourceRoot": "libs/tags/src",
      "prefix": "rpd10",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/tags/tsconfig.lib.json",
              "libs/tags/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!libs/tags/**/*"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/tags/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": [
            "coverage/libs/tags"
          ]
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      }
    },
    "blog-data": {
      "projectType": "library",
      "root": "libs/blog-data",
      "sourceRoot": "libs/blog-data/src",
      "prefix": "rpd10",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/blog-data/tsconfig.lib.json",
              "libs/blog-data/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!libs/blog-data/**/*"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/blog-data/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": [
            "coverage/libs/blog-data"
          ]
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    }
  },
  "defaultProject": "web"
}